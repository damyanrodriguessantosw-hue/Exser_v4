local function CarregarExecutor()
    LimparConteudo()
    local y = 10
    
    CriarLabel("=== EXECUTOR DE SCRIPTS ===", y); y = y + 30
    
    local scriptsBanidos = {"dominantultimate", "SentinelPrime", "RC7"}
    
    CriarTextbox("Executar Script", function(script)
        -- VERIFICAR SCRIPTS BANIDOS
        local banido = false
        for _, padrao in ipairs(scriptsBanidos) do
            if script:find(padrao) then
                banido = true
                break
            end
        end
        
        if banido then
            CriarLabel("üö´ SCRIPT BANIDO!", y)
            EnviarLog("Tentativa de script banido")
            game.Players.LocalPlayer:Kick("Script proibido!")
            return
        end
        
        -- EXECUTAR
        local func, err = loadstring(script)
        if func then
            local suc, erro = pcall(func)
            if suc then
                CriarLabel("‚úÖ Script executado!", y)
                EnviarLog("Script executado")
            else
                CriarLabel("‚ùå Erro: " .. tostring(erro), y)
            end
        else
            CriarLabel("‚ùå Erro: " .. tostring(err), y)
        end
    end, y); y = y + 60
    
    CriarBotao("Limpar Console", function()
        for i = 1, 50 do print("") end
        CriarLabel("‚úì Console limpo!", y)
    end, y); y = y + 45
end

